@if (ViewBag.ErrorTitle == null)
{
    <h3>
        An error occured while processing your request.
        Support team will be notified.
    </h3>
    <h4>Contact support team support@pragimtech.com</h4>
}
else
{
    <h1 class="text-danger">@ViewBag.ErrorTilte</h1>
    <h6 class="text-danger">@ViewBag.ErrorMessage</h6>

}



<hr />
<h3>Exception Details</h3>

<div class=" alert alert-danger">
    <h3>
        Exception Path
    </h3>
    <hr />
    <p>
        @ViewBag.ExceptionPath
    </p>
</div>

<div class=" alert alert-danger">
    <h3>
        Exception Message
    </h3>
    <hr />
    <p>
        MessagePath
    </p>
</div>

<div class=" alert alert-danger">
    <h3>
        Exception StackTrace
    </h3>
    <hr />
    <p>
        @ViewBag.ExceptionStackTrace
    </p>
</div>


